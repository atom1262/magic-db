<!DOCTYPE html>
<html>
<head>
  <title>MagicDb</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= render "partials/nav" %>

    <div id="flash">
      <% flash.each do |key, value| %>
        <div id="flash_<%= key %>"><%=h value %></div>
      <% end %>
        </div>
    </div>

<%= yield %>

</body>
</html>
