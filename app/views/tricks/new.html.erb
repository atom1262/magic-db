<h1>Add New Trick</h1>

<div id="new-trick-form">
  <%= form_for @trick do |form| %>

    <p>
      <%= form.text_field :name, placeholder: 'Trick Name' %>
    </p>

    <p>
      <%= form.text_area :description, placeholder: 'Trick Description' %>
    </p>

    <p>
      <%= form.text_field :youtube_url, placeholder: 'Youtube URL' %>
    </p>

    <p>
      <%= form.submit "Upload Trick" %>
    </p>

  <% end %>
</div>
